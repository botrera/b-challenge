{
  "name": "project-name-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "build-docs": "node_modules/.bin/redoc-cli bundle ./documentation/openapi.yaml",
    "dev": "nodemon",
    "docker-compose": "docker-compose up",
    "docker-compose-build": "docker-compose up --build",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate-undo": "node_modules/.bin/sequelize db:migrate:undo",
    "migrate-test-db": "ENV=test node_modules/.bin/sequelize db:migrate",
    "migrate-test-db-undo": "ENV=test node_modules/.bin/sequelize db:migrate:undo",
    "watch:server": "nodemon \"./dist/bundle.js\" --watch \"./build\" ",
    "watch:build": "webpack --watch",
    "start": "NODE_PATH=dist/ node dist/src/server.js",
    "test": "env $(cat .env.test | xargs) ENV=test jest --verbose=false --coverage --detectOpenHandles --forceExit",
    "lint": "eslint -c .eslintrc.json --ext .ts '{src,test}/**/*.ts'",
    "format": "prettier -w -c ./.prettierrc '{src,test}/**/*.ts'",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@aws-sdk/lib-storage": "^3.150.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/integrations": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@typescript-eslint/scope-manager": "^5.25.0",
    "@typescript-eslint/typescript-estree": "^5.25.0",
    "@typescript-eslint/utils": "^5.25.0",
    "aws-sdk": "^2.1198.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "~1.20.0",
    "class-transformer": "^0.5.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "debug": "~4.3.4",
    "dotenv": "^16.0.1",
    "email-validator": "^2.0.4",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.0",
